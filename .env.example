# Alpaca (paper or live)
APCA_API_KEY_ID=
APCA_API_SECRET_KEY=
APCA_API_BASE_URL=https://paper-api.alpaca.markets

# OpenRouter
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openai/gpt-4o

# Bot
TRADING_INTERVAL_MINUTES=60
DRY_RUN=true
SYMBOLS=AAPL,GOOGL,MSFT

# Research mode: expand symbols from movers + most actives, include run history and market conditions in decisions
# RESEARCH_MODE=false
# RESEARCH_SYMBOLS_CAP=10
# MARKET_CONDITION_SYMBOLS=SPY,QQQ

# Optional: Gotify push notifications (when a trade is executed)
# GOTIFY_SERVER_URL=https://gotify.example.com
# GOTIFY_APP_TOKEN=

# Optional: max notional per order
# MAX_ORDER_NOTIONAL=1000

# Optional: HTTP server port (default: 4108)
# PORT=4108

# Optional: data dir for SQLite (default: data)
# DATA_DIR=data
